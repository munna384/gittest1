pipeline{
    agent any
    stages{
        stage("version"){
            steps{
                sh "cat /etc/os-release"
                
            }
        }
        stage("scanner"){
            steps {
                sh "/opt/sonar-scanner-4.6.0.2311-linux/bin/sonar-scanner -X -Dproject.settings=sonar-project.properties"
            }
        }
    }
}
